	year = {2010},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/prot.22711},
	keywords = {NMR, molecular mechanics, protein dynamics, molecular dynamics simulation, quantum mechanics, dihedral, rotamer, side chain},
	pages = {1950--1958},
	file = {Full Text PDF:/home/ccavende/Zotero/storage/2PFG2KGP/Lindorff-Larsen et al. - 2010 - Improved side-chain torsion potentials for the Amb.pdf:application/pdf;Snapshot:/home/ccavende/Zotero/storage/PHMHPK8G/prot.html:text/html},
