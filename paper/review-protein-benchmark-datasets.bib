	note = {tex.ids= Robustelli:2018:PNAS, Robustelli:2018:Proc.Natl.Acad.Sci.
publisher: Proceedings of the National Academy of Sciences},
	keywords = {computer simulations, intrinsically disordered proteins, protein dynamics},
	pages = {E4758--E4766},
	file = {Full Text PDF:/home/ccavende/Zotero/storage/HCJRA4ZL/Robustelli et al. - 2018 - Developing a molecular dynamics force field for bo.pdf:application/pdf;Full Text PDF:/home/ccavende/Zotero/storage/3US8IH8K/Robustelli et al. - 2018 - Developing a molecular dynamics force field for bo.pdf:application/pdf;Full Text PDF:/home/ccavende/Zotero/storage/Y9BBUXK9/Robustelli et al. - 2018 - Developing a molecular dynamics force field for bo.pdf:application/pdf;Robustelli et al_2018_Developing a molecular dynamics force field for both folded and disordered.pdf:/home/ccavende/Zotero/storage/TGVZG4SA/Robustelli et al_2018_Developing a molecular dynamics force field for both folded and disordered.pdf:application/pdf;Snapshot:/home/ccavende/Zotero/storage/RXLJYKUZ/1800690115.html:text/html;Snapshot:/home/ccavende/Zotero/storage/V5GMVPVS/E4758.html:text/html;Snapshot:/home/ccavende/Zotero/storage/5NRA4DDR/1800690115.html:text/html},
