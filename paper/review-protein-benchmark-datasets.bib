@article{schwalbe_predictive_2014,
	title = {Predictive {Atomic} {Resolution} {Descriptions} of {Intrinsically} {Disordered} {hTau40} and α-{Synuclein} in {Solution} from {NMR} and {Small} {Angle} {Scattering}},
