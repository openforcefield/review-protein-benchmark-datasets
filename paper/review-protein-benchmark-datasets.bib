	keywords = {NMR, Ensemble, Intrinsically disordered protein, Scalar coupling},
	pages = {299--307},
	file = {Full Text PDF:/home/ccavende/Zotero/storage/MNU5I7WT/Gapsys et al. - 2015 - Improved validation of IDP ensembles by one-bond C.pdf:application/pdf},
}

