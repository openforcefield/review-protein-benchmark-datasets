@article{van_gunsteren_validation_2018,
	title = {Validation of {Molecular} {Simulation}: {An} {Overview} of {Issues}},
	volume = {57},
	issn = {1521-3773},
	shorttitle = {Validation of {Molecular} {Simulation}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/anie.201702945},
	doi = {10.1002/anie.201702945},
	abstract = {Computer simulation of molecular systems enables structure–energy–function relationships of molecular processes to be described at the sub-atomic, atomic, supra-atomic, or supra-molecular level. To interpret results of such simulations appropriately, the quality of the calculated properties must be evaluated. This depends on the way the simulations are performed and on the way they are validated by comparison to values Qexp of experimentally observable quantities Q. One must consider 1) the accuracy of Qexp, 2) the accuracy of the function Q(rN) used to calculate a Q-value based on a molecular configuration rN of N particles, 3) the sensitivity of the function Q(rN) to the configuration rN, 4) the relative time scales of the simulation and experiment, 5) the degree to which the calculated and experimental properties are equivalent, and 6) the degree to which the system simulated matches the experimental conditions. Experimental data is limited in scope and generally corresponds to averages over both time and space. A critical analysis of the various factors influencing the apparent degree of (dis)agreement between simulations and experiment is presented and illustrated using examples from the literature. What can be done to enhance the validation of molecular simulation is also discussed.},
	language = {en},
	number = {4},
	urldate = {2023-03-01},
	journal = {Angewandte Chemie International Edition},
	author = {van Gunsteren, Wilfred F. and Daura, Xavier and Hansen, Niels and Mark, Alan E. and Oostenbrink, Chris and Riniker, Sereina and Smith, Lorna J.},
	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/anie.201702945},
	keywords = {Monte Carlo simulation, molecular dynamics simulation, computer chemistry, experimental data, pseudo-validation},
	pages = {884--902},
	file = {Snapshot:/home/ccavende/Zotero/storage/ADTQF67I/anie.html:text/html;Submitted Version:/home/ccavende/Zotero/storage/JQ2RFIU2/van Gunsteren et al. - 2018 - Validation of Molecular Simulation An Overview of.pdf:application/pdf},
}

