}

@article{fraser_accessing_2011,
	title = {Accessing protein conformational ensembles using room-temperature {X}-ray crystallography},
	volume = {108},
	url = {https://www.pnas.org/doi/full/10.1073/pnas.1111325108},
	doi = {10.1073/pnas.1111325108},
	abstract = {Modern protein crystal structures are based nearly exclusively on X-ray data collected at cryogenic temperatures (generally 100 K). The cooling process is thought to introduce little bias in the functional interpretation of structural results, because cryogenic temperatures minimally perturb the overall protein backbone fold. In contrast, here we show that flash cooling biases previously hidden structural ensembles in protein crystals. By analyzing available data for 30 different proteins using new computational tools for electron-density sampling, model refinement, and molecular packing analysis, we found that crystal cryocooling remodels the conformational distributions of more than 35\% of side chains and eliminates packing defects necessary for functional motions. In the signaling switch protein, H-Ras, an allosteric network consistent with fluctuations detected in solution by NMR was uncovered in the room-temperature, but not the cryogenic, electron-density maps. These results expose a bias in structural databases toward smaller, overpacked, and unrealistically unique models. Monitoring room-temperature conformational ensembles by X-ray crystallography can reveal motions crucial for catalysis, ligand binding, and allosteric regulation.},
	number = {39},
	urldate = {2023-02-28},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Fraser, James S. and van den Bedem, Henry and Samelson, Avi J. and Lang, P. Therese and Holton, James M. and Echols, Nathaniel and Alber, Tom},
	month = sep,
	year = {2011},
	pmid = {21918110},
	note = {tex.ids= Fraser:2011:Proc.Natl.Acad.Sci.USA
publisher: Proceedings of the National Academy of Sciences},
	keywords = {energy landscape, protein conformational dynamics, qFit, Ringer},
	pages = {16247--16252},
	file = {Fraser et al_2011_Accessing protein conformational ensembles using room-temperature X-ray.pdf:/home/ccavende/Zotero/storage/UDZXD4H2/Fraser et al_2011_Accessing protein conformational ensembles using room-temperature X-ray.pdf:application/pdf;Full Text PDF:/home/ccavende/Zotero/storage/2RWUML6L/Fraser et al. - 2011 - Accessing protein conformational ensembles using r.pdf:application/pdf;Snapshot:/home/ccavende/Zotero/storage/BY8QZ2X3/16247.html:text/html},
