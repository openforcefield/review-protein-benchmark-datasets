	year = {2007},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1110/ps.062557707},
	keywords = {1H/19F and 19F/1H heteronuclear NOE, backbone thioester exchange, NMR, pentafluorophenylalanine, villin headpiece},
	pages = {14--19},
	file = {Full Text PDF:/home/ccavende/Zotero/storage/5JFCTMQF/Cornilescu et al. - 2007 - Solution structure of a small protein containing a.pdf:application/pdf;Snapshot:/home/ccavende/Zotero/storage/EE7T5C52/ps.html:text/html},
