@article{vogeli_nuclear_2014,
	title = {The nuclear {Overhauser} effect from a quantitative perspective},
	volume = {78},
	issn = {0079-6565},
	url = {https://www.sciencedirect.com/science/article/pii/S0079656513001003},
	doi = {10.1016/j.pnmrs.2013.11.001},
	abstract = {The nuclear Overhauser enhancement or effect (NOE) is the most important measure in liquid-state NMR with macromolecules. Thus, the NOE is the subject of numerous reviews and books. Here, the NOE is revisited in light of our recently introduced measurements of exact nuclear Overhauser enhancements (eNOEs), which enabled the determination of multiple-state 3D protein structures. This review encompasses all relevant facets from the theoretical considerations to the use of eNOEs in multiple-state structure calculation. Important aspects include a detailed presentation of the relaxation theory relevant for the nuclear Overhauser effect, the estimation of the correction for spin diffusion, the experimental determination of the eNOEs, the conversion of eNOE rates into distances and validation of their quality, the distance-restraint classification and the protocols for calculation of structures and ensembles.},
	urldate = {2024-01-24},
	journal = {Progress in Nuclear Magnetic Resonance Spectroscopy},
	author = {Vögeli, Beat},
	month = apr,
	year = {2014},
	keywords = {NOE, eNOE, Nuclear Overhauser effect},
	pages = {1--46},
	file = {ScienceDirect Snapshot:/home/ccavende/Zotero/storage/W5QRZQ5K/S0079656513001003.html:text/html;Submitted Version:/home/ccavende/Zotero/storage/FR2LQGZX/Vögeli - 2014 - The nuclear Overhauser effect from a quantitative .pdf:application/pdf},
}

@article{vogeli_exact_2009,
