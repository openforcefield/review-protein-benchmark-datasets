}

@article{peintner_pushing_2022,
	title = {Pushing the {Limits} of {Characterising} a {Weak} {Halogen} {Bond} in {Solution}},
	volume = {28},
	copyright = {© 2021 The Authors. Chemistry - A European Journal published by Wiley-VCH GmbH},
