	month = feb,
	year = {2018},
	pmid = {29294268},
	note = {tex.ids= Chen:2018:J.Chem.TheoryComput.
publisher: American Chemical Society},
	pages = {1009--1019},
	file = {ACS Full Text Snapshot:/home/ccavende/Zotero/storage/4ANRPMGI/acs.jctc.html:text/html;Full Text:/home/ccavende/Zotero/storage/HBIFDZA6/Chen et al. - 2018 - Ab Initio Prediction of NMR Spin Relaxation Parame.pdf:application/pdf;Full Text PDF:/home/ccavende/Zotero/storage/I27MC2SQ/Chen et al. - 2018 - Ab Initio Prediction of NMR Spin Relaxation Parame.pdf:application/pdf},
