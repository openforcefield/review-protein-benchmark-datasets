	note = {Publisher: American Chemical Society},
	pages = {1432--1435},
	file = {ACS Full Text Snapshot:/home/ccavende/Zotero/storage/WJSHHZZP/ja512593s.html:text/html;Full Text PDF:/home/ccavende/Zotero/storage/IDGARLVP/Lee et al. - 2015 - Quantitative Residue-Specific Protein Backbone Tor.pdf:application/pdf},
}

@article{li_high_2015,
