@article{van_gunsteren_deriving_2016,
	title = {Deriving {Structural} {Information} from {Experimentally} {Measured} {Data} on {Biomolecules}},
	volume = {55},
	copyright = {© 2016 Wiley-VCH Verlag GmbH \& Co. KGaA, Weinheim},
